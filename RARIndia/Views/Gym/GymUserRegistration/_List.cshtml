@using RARIndia.ViewModel
@model GymUserRegistrationListViewModel
@{
    Layout = null;
    string actionName = ViewContext.Controller.ValueProvider.GetValue("action").RawValue.ToString();
    string controllerName = ViewContext.RouteData.Values["controller"].ToString();
}
<div class="row">
    <div class="col-sm-12 col-md-6">
        @Html.Partial("~/Views/Shared/Datatable/_DatatableShowResult.cshtml", Model.PageListViewModel.RecordPerPage)
    </div>
    <div class="col-sm-12 col-md-6">
        @Html.Partial("~/Views/Shared/Datatable/_DatatableSearchData.cshtml", Model.PageListViewModel.SearchBy)
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <table class="table table-striped table-bordered column-rendering dataTable" id="DataTables_Table_1" role="grid" aria-describedby="DataTables_Table_1_info" style="width: 1223px;">
            <thead>
                <tr role="row">
                    @*<th class="sorting" data-columnname="">Registration Code</th>*@
                    <th class="sorting" data-columnname="">Name</th>
                    <th class="sorting" data-columnname="">Contact Number</th>
                    <th class="sorting" data-columnname="">Age</th>
                    <th style="width:10%;">Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model?.GymUserRegistrationList)
                {
                    <tr>
                        @*<td>@item.RegistrationCode</td>*@
                        <td>@item.FirstName @item.LastName</td>
                        <td>@item.ContactNumber</td>
                        <td>@item.Age</td>
                        <td style="text-align:center">
                            @*<a href="../@controllerName/GymPrintUserRegistration?gymUserRegistrationId=@item.GymUserRegistrationId" title="Print Receipt"><i class="ft-edit-1"></i></a>*@  @*|*@
                            <a href="../@controllerName/GymPrintUserRegistration?gymUserRegistrationId=@item.GymUserRegistrationId" title="Print Receipt"><i class="ft-printer"></i></a>  @*|*@
                            @*<a href="../@controllerName/Delete?countryIds=@item.GymUserRegistrationId" title="Delete"><i class="ft-delete"></i></a>*@
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
@Html.Partial("~/Views/Shared/Datatable/_DatatablePagination.cshtml", Model.PageListViewModel)
