@using RARIndia.Utilities.Helper;
@using RARIndia.Utilities.Constant;
@using RARIndia.Model;
@{
    ViewBag.Title = "Dashboard";
    UserModel userData = RARIndiaSessionHelper.GetDataFromSession<UserModel>(RARIndiaConstant.UserDataSession);
}
<section id="minimal-statistics-bg">
    <div class="row">
        @foreach (UserModuleModel item in userData?.ModuleList)
        {
            <div class="col-xl-3 col-md-6 col-12">
                <div class="card bg-info">
                    <div class="card-content">
                        <div class="card-body" style="padding-top: 33px; padding-bottom: 33px">
                            <div class="media d-flex">
                                <div class="media-body text-white text-center">
                                    <span>@item.ModuleName</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</section>
